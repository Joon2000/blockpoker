{
  "canisters": {
    "backend": {
      "main": "canisters/backend/main.mo"
    },
    "randomNumber": {
      "main": "canisters/randomNumber/main.mo",
      "type": "motoko"
    },
    "Turn": {
      "main": "canisters/Turn/main.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": ["randomNumber", "Turn"],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": ["dist/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    }
  },
  "version": 1
}
