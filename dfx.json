{
  "canisters": {
    "poker": {
      "dependencies": ["random_number"],
      "main": "backend/poker/main.mo",
      "type": "motoko"
    },
    "random_number": {
      "main": "backend/random_number/main.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": ["poker", "random_number", "Turn"],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": ["dist/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:5943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    }
  },
  "version": 1
}
